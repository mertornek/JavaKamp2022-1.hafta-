

public class Main {

	public static void main(String[] args) {
		
		int number = 1;
		
		for(int i=2; i< number; i++ ) {
			
			
			
			
			if(number % i == 0 ) {
				System.out.println("Asal Değil");
				break;
			}
			else if(number % i != 0 | number == 2 ){
				System.out.println("Asal");
				break;
				}
			
			else {
				System.out.println("Asal sayı değil");
				break;
				
			}
			
		
		
		}
		
		}
		
		
		
		
		

	}

